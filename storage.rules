rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /shareProofs/{schoolId}/{fileName} {
      allow read: if true;
      allow write: if request.resource.size < 10 * 1024 * 1024;
    }

    match /{allPaths=**} {
      allow read: if true;
      allow write: if false;
    }
  }
}

