rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

    match /schools/{schoolId} {
      allow read: if true;
      allow write: if false;
    }

    match /sharer/{documentId} {
      allow read: if true;
      allow create: if true;
      allow update, delete: if false;
    }

    match /{document=**} {
      allow read: if true;
      allow write: if false;
    }
  }
}

